{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "QMB5200"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"LocalPCLinkedServiceViewsCSV_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LocalPCLinkedServiceViewsCSV'"
		},
		"Mongo_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Mongo'"
		},
		"QMB5200_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'QMB5200'"
		},
		"QMB5200_1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'QMB5200_1'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"LocalPCLinkedServiceViewsCSV_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\myMongoData"
		},
		"LocalPCLinkedServiceViewsCSV_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Milana Saradzic"
		},
		"Mongo_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\myMongoData"
		},
		"Mongo_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "milana"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"LocalPCLinkedServiceViewsCSV_password": {
						"value": "[parameters('LocalPCLinkedServiceViewsCSV_password')]"
					},
					"QMB5200_connectionString": {
						"value": "[parameters('QMB5200_connectionString')]"
					},
					"QMB5200_1_connectionString": {
						"value": "[parameters('QMB5200_1_connectionString')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"Mongo_password": {
						"value": "[parameters('Mongo_password')]"
					},
					"LocalPCLinkedServiceViewsCSV_properties_typeProperties_host": {
						"value": "[parameters('LocalPCLinkedServiceViewsCSV_properties_typeProperties_host')]"
					},
					"LocalPCLinkedServiceViewsCSV_properties_typeProperties_userId": {
						"value": "[parameters('LocalPCLinkedServiceViewsCSV_properties_typeProperties_userId')]"
					},
					"Mongo_properties_typeProperties_host": {
						"value": "[parameters('Mongo_properties_typeProperties_host')]"
					},
					"Mongo_properties_typeProperties_userId": {
						"value": "[parameters('Mongo_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}